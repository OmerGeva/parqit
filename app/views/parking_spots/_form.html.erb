<div class="col-8">
  <%= simple_form_for [account, parking_spot] do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :address %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "Add Spot", class: "btn btn-primary btn-block mb-5" %>
  <% end %>
</div>
